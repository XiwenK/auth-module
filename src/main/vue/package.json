{
  "name": "groups",
  "version": "0.0.2",
  "description": "Groups application",
  "productName": "Groups",
  "author": "manunin <alexandr.manunin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@pinia/testing": "^0.0.16",
    "@quasar/extras": "^1.0.0",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "pinia": "^2.0.33",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.3.0-beta.17",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^29.5.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "vitest": "^0.29.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "testEnvironment": "jsdom"
  }
}
